{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Hivenet Docs",
  "colors": {
    "primary": "#D46E35",
    "light": "#D46E35",
    "dark": "#D46E35"
  },
  "favicon": "logo/hivenet_favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "get-started/quickstart",
              "get-started/development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "Tutorials",
        "groups": [
          {
            "group": "Guides",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "More guides",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      },
      {
        "tab": "Changelog",
        "groups": [
          {
            "group": "Changelog",
            "pages": [
              "changelog"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Help Center",
          "href": "https://help.hivenet.com/en/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.com/invite/p2CwYSKsQU",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://compute.hivenet.com/computes-blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@hivenet.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/hivenet_cloud",
      "linkedin": "https://www.linkedin.com/company/hivenetdistributedcloud/"
    }
  }
}
